{
  "compilerOptions": {
    "target": "ES2023",
    "module": "ES2022",
    "lib": ["ES2023"],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "astal/gtk3/jsx-runtime"
  }
}
